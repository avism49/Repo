<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
       xmlns:context="http://www.springframework.org/schema/context"
      
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	   		http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
	   		http://www.springframework.org/schema/context
	   		http://www.springframework.org/schema/context/spring-context-4.0.xsd">
	   <bean id="uno" class="com.learnspring.springbeans.SetterInjectionBean">
	   <property name="salary" value="50000"></property>
	   <property name="name" value="avi"></property>
	   </bean>
	   		
	   		<bean id="conju" class="com.learnspring.springbeans.ConstInject">
	   		<constructor-arg value="science"/>
	   		<constructor-arg value="hyderabad"/>
	   		</bean>
	   		</beans>
